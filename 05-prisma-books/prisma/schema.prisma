generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

// n:n

model Book {
	id		Int		@id @default(autoincrement()) @db.UnsignedInt
	title	String
	pages	Int		@db.UnsignedInt
	authors Author[]
}

model Author {
	id		Int		@id @default(autoincrement()) @db.UnsignedInt
	name	String
	books 	Book[]
}


